{
    "extends": "./tsconfig",
    "compilerOptions": {
        "types": ["cypress", "node"],
        "typeRoots": ["./src/types/index.d.ts", "./node_modules/@types"]
    },
    "include": [
        "cypress.config.ts",
        "cypress",
        "src/types/*.ts",
        "_static/types/**/*.ts",
        "**/*.ts",
        "**/*.tsx",
        "./src"
    ],
    "exclude": ["node_modules"]
}
